pipeline {
    agent any
    stages{
      stage ('pull'){
       steps{
         git 'https://github.com/aatishvaidya10-oss/studentapp-ui.git'
       }
      }
      stage ('Build'){
       steps {
         sh '/opt/maven/bin/mvn clean package'
       }
      }
       stage ('Test'){
        steps {
          echo 'Tested'
       }
      }
       stage ('Deploy'){
        steps {
          echo 'deployed'
       }
      }

    }
}
