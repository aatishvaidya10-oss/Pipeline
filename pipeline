pipeline {
    agent any
    stages{
      stage ('pull'){
       steps{
         git 'https://github.com/aatishvaidya10-oss/studentapp-ui.git'
       }
      }
      stage {
       steps ('Build'){
         sh '/opt/maven/bin/mvn clean package'
       }
      }
       stage {
        steps ('Test'){
          echo 'Tested'
       }
      }
       stage {
        steps ('Deploy'){
          echo 'deployed'
       }
      }

    }
}
